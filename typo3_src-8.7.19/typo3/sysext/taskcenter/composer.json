{
	"name": "typo3/cms-taskcenter",
	"type": "typo3-cms-framework",
	"description": "The Task Center is the framework for a host of other extensions, see below.",
	"homepage": "https://typo3.org",
	"license": ["GPL-2.0-or-later"],
	"authors": [{
		"name": "TYPO3 Core Team",
		"email": "typo3cms@typo3.org",
		"role": "Developer"
	}],
	"config": {
		"sort-packages": true
	},
	"require": {
		"php": ">=7.0.0 <=7.2.99",
		"typo3/cms-core": "8.7.19"
	},
	"suggest": {
		"typo3/cms-sys-action": "8.7.19"
	},
	"conflict": {
		"typo3/cms": "*"
	},
	"replace": {
		"taskcenter": "*"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "taskcenter"
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\Taskcenter\\": "Classes/"
		}
	}
}
