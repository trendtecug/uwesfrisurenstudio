      <target state="translated">Раз в сутки</target></trans-unit>
      <trans-unit id="index_config.timer_frequency.I.2" xml:space="preserve" approved="yes">
        <source>Every week</source>
      <target state="translated">Раз в неделю</target></trans-unit>
      <trans-unit id="index_config.recordsbatch" xml:space="preserve" approved="yes">
